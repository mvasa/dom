package groupw;

/**
 *
 * @author chris_000
 */
public class NodePoint {

    public int height;
    public int width;

    NodePoint(int h, int w) {
        this.height = h;
        this.width = w;
    }

    public void setHeight(int y) {
        height = y;
    }

    public int getHeight() {
        return this.height;
    }

    void setWidth(int x) {
        width = x;
    }

    public int getWidth() {
        return this.width;
    }

}
